<div class="container">

  <h1><%= title %></h1>

    <h2>Users: </h2>
    <% for (i = 0; i < users.length; i++)
    { %>
      <%= users[i] %>
      <% } %>

    <form action="/ta02/addUser" method="POST">
        Add a User: <input type="text" name="u1">
        <br>
        <button type="submit">Add</button>
    </form>
  
</div>

<div class="container">

  <h1><%= title %></h1>
    <form action="/ta02/removeUser" method="POST">
        Remove a User: <input type="text" name="u2">
        <br>
        <button type="submit">Remove</button>
    </form>
  
</div>